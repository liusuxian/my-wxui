@font-face {
  font-family: "myCollapse";
  src: url("data:font/ttf;charset=utf-8;base64,")
    format("truetype");
  font-weight: normal;
}

.my-collapse-icon {
  font-family: "myCollapse" !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.my-collapse-icon__arrowdown:before {
  content: "\e61e";
}

.my-collapse-icon__arrowdown {
  transform: rotate(0);
  transform-origin: center center;
  transition: all 0.3s;
  color: #333;
  font-size: 22px;
}

.my-collapse-icon__arrowdown-active {
  transform: rotate(180deg);
  transform-origin: center center;
}

.my-collapse {
  background-color: transparent;
}

.my-collapse-head {
  position: relative;
  z-index: 2;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 8rpx;
  color: #333;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.my-collapse-head__opacity {
  opacity: 0.6;
}

.my-collapse-body_box {
  transition: all 0.25s;
  overflow: hidden;
}

.my-collapse-body {
  transition: all 0.25s;
  overflow: hidden;
  position: relative;
  z-index: 1;
}

.my-collapse-body__transform {
  opacity: 0;
  visibility: hidden;
  -webkit-transform: translateY(-40%);
  transform: translateY(-40%);
}

.my-collapse-body__show {
  opacity: 1;
  visibility: visible;
  -webkit-transform: translateY(0);
  transform: translateY(0);
}
